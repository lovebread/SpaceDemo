<h2><%= @user.username %>'s Photos</h2>

<% if is_logged_in? && logged_in_user == @user %>
  <p><%= link_to 'Upload a new photo', {:controller => 'user_photos',
					 					:action =>'new',
					 					:user_id => @logged_in_user} %>
  </p>
<% end %>

<%= render :partial => 'photos/page_links' %>

<ul id="photos">
  <%= render :partial => 'photos/photo', :collection => @photos %>
</ul>

<%= render :partial => 'photos/page_links' %>
